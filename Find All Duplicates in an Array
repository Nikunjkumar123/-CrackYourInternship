class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int>ans;
        map<int,int>mpp;
        map<int,int>::iterator it;
        for(int i=0;i<nums.size();i++){
            mpp[nums[i]]++;
        }
        for(it=mpp.begin();it!=mpp.end();it++){
            if(it->second>1){
                ans.push_back(it->first);
            }
        }
        return ans;
    }
};
