class Solution {
public:
    int subarraysDivByK(vector<int>& nums, int k) {
        int prefix=0;
        unordered_map<int,int>mpp;
        int ans=0;mpp[0]=1;
        for(int i=0;i<nums.size();i++){
            prefix=(prefix+nums[i]%k+k)%k;
            ans+=mpp[prefix];
            mpp[prefix]++;
            
        }
        
        // for(it=mpp.begin();it!=mpp.end();it++){
        //     ans+=(it->second);
        // }
        return ans;
    }
};
