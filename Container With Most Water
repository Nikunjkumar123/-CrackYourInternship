class Solution {
public:
    int maxArea(vector<int>& height) {
        int ans=0;int l=0,r=height.size()-1;
        while(l<=r){
            int a=min(height[l],height[r])*(r-l);
            ans=max(ans,a);
            if(height[l]>height[r]) r--;

            else{ l++;}
        }
        return ans;
    }
};
